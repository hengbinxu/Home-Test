{
  "driver": {
    "browser": "chrome",
    "remote_url": "http://127.0.0.1:4444",
    "driver_path": null,
    "browser_options": [
      "--disable-gpu",
      "--no-sandbox",
      "--start-maximized",
      "--enable-logging"
    ],
    "capabilities": {
      "goog:loggingPrefs": {
        "performance": "ALL",
        "browser": "ALL",
        "client": "ALL",
        "server": "ALL",
        "driver": "ALL"
      }
    },
    "experimental_options": { "mobileEmulation": { "deviceName": "iPhone X" } },
    "extension_paths": [],
    "webdriver_kwargs": {},
    "page_load_wait_time": 10,
    "implicitly_wait": 5
  },
  "log": {
    "screenshot_on": true
  },
  "viewport": {
    "maximize": true,
    "width": 1440,
    "height": 900,
    "orientation": "portrait"
  }
}
